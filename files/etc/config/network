
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '10.59.72.119'
	option netmask '255.255.255.128'
	option gateway '10.59.72.1'
	option broadcast '10.59.72.127'
	option dns '10.129.128.151 10.137.130.151'
	option delegate '0'