
config qbittorrent 'main'
	option user 'root'
	option BTProtocol 'Both'
	option CategoryChanged 'false'
	option DefaultSavePathChanged 'true'
	option CategorySavePathChanged 'true'
	option Encryption '0'
	option MaxRatioAction '0'
	option CSRFProtection 'false'
	option SeedChokingAlgorithm 'FastestUpload'
	option Enabled 'false'
	option profile '/opt'
	option DisableAutoTMMByDefault 'false'
	option Locale 'en'
	option LSD 'true'
	option AuthSubnetWhitelistEnabled 'true'
	list AuthSubnetWhitelist '10.59.72.0/24'
	option LocalHostAuth 'false'
	option Port '9898'
	option ClickjackingProtection 'false'
	option HostHeaderValidation 'false'
	option enabled '0'
	option SavePath '/mnt/sda4/Downloads'
	option TempPath '/mnt/sda4/Downloads/temp'

